{
	"manifest_version": 2,
	"name": "Grammer Ungooder",
	"version": "1.0",
	"description": "Fix all that pesky 'correct' grammer",
	"icons":
	{
		"16": "icon16.png",
		"48": "icon48.png",
		"128": "icon128.png"
	},
	"content_scripts":
	[
		{
			"matches": ["*://*/*"],
			"js": ["content_script.js"],
			"run_at": "document_end"
		}
	]
}
